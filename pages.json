{
	// 主包配置
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "个人中心"
			}
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "智慧园区小程序",
				"navigationStyle":"custom"
			}
		},
		{
			"path": "pages/mailList/mailList",
			"style": {
				"navigationBarTitleText": "通讯录"
			}
		}
		
	],
	// 分包配置
	"subPackages":[{
		"root": "page", // 当前包的根目录文件
		"pages":[
			{  // 当前包的页面配置
				"path": "equipmentInspection/equipmentInspection/equipmentRepairService",
				"style": {
					"navigationBarTitleText": "设备报修"
				}
			},
			{  // 当前包的页面配置
				"path": "login/accountNumber/accountNumber",
				"style": {
					"navigationBarTitleText": "登录"
				}
			},
			{  // 当前包的页面配置
				"path": "login/wx/wx",
				"style": {
					"navigationBarTitleText": "登录"
				}
			}
			
		]
	}],
	"preloadRule": {
		"page/equipmentInspection/equipmentInspection/equipmentRepairService": {// 分包中的主目录
			"network": "all",// 默认all，即适用所有网络
			"packages": ["__APP__"]// 进入packagePages/index/index后，加载整个项目
		},
		"page/login/accountNumber/accountNumber": {// 分包中的主目录
			"network": "all",// 默认all，即适用所有网络
			"packages": ["__APP__"]// 进入packagePages/index/index后，加载整个项目
		},
		"page/login/wx/wx": {// 分包中的主目录
			"network": "all",// 默认all，即适用所有网络
			"packages": ["__APP__"]// 进入packagePages/index/index后，加载整个项目
		}
	},
	
	// 全局配置
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "智慧园区小程序",
		"navigationBarBackgroundColor": "#FFFFFF",
		"backgroundColor": "#F8F8F8",
		
		"usingComponents": {
		    "van-row": "/wxcomponents/vant/dist/row/index",
		    "van-col": "/wxcomponents/vant/dist/col/index",
		    "van-icon": "/wxcomponents/vant/dist/icon/index",
		    "van-button": "/wxcomponents/vant/dist/button/index",
			"van-cell": "/wxcomponents/vant/dist/cell/index",
			"van-cell-group": "/wxcomponents/vant/dist/cell-group/index",
			"van-field": "/wxcomponents/vant/dist/field/index",
			"van-grid": "/wxcomponents/vant/dist/grid/index",
			"van-grid-item": "/wxcomponents/vant/dist/grid-item/index",
			"van-popup": "wxcomponents/vant/dist/popup/index",
			"van-checkbox": "wxcomponents/vant/dist/checkbox/index",
			"van-checkbox-group": "wxcomponents/vant/dist/checkbox-group/index"
			
		}
	},
	// 底部导航栏配置
	"tabBar": {
		"list": [{
			"pagePath": "pages/index/index",
			"text": "首页",
			"iconPath": "static/image/index/indexW@2x.png",
			"selectedIconPath": "static/image/index/index2@2x.png"
		},
		{
			"pagePath": "pages/mailList/mailList",
			"text": "通讯录",
			"iconPath": "static/image/index/通讯未选中@2x.png",
			"selectedIconPath": "static/image/index/通讯录@2x.png"
		},
		{
			"pagePath": "pages/my/my",
			"text": "我的",
			"iconPath": "static/image/index/我的未选中@2x.png",
			"selectedIconPath": "static/image/index/我的@2x.png"
		}]
	}
}
