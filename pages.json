{
	// 主包配置
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/smartStreet/index/index",
			"style": {
				"navigationBarTitleText": "智慧园区小程序",
				"navigationStyle":"custom"
			}
		},
		{
			"path": "pages/smartStreet/mailList/mailList",
			"style": {
				"navigationBarTitleText": "通讯录"
			}
		},
		{
			"path": "pages/smartStreet/my/my",
			"style": {
				"navigationBarTitleText": "个人中心"
			}
		}
		
	],
	// 分包配置
	"subPackages":[{
		"root": "page/smartrtStreet_page", // 当前包的根目录文件
		"pages":[
			{  // 当前包的页面配置
				"path": "equipmentInspection/equipmentInspection/equipmentRepairService",
				"style": {
					"navigationBarTitleText": "设备报修"
				}
			},
			{  // 当前包的页面配置
				"path": "login/accountNumber/accountNumber",
				"style": {
					"navigationBarTitleText": "登录"
				}
			},
			{  // 当前包的页面配置
				"path": "publicFeedback/publicFeedback/publicFeedback",
				"style": {
					"navigationBarTitleText": "社情民意反馈"
				}
			},
			{  // 当前包的页面配置
				"path": "employmentInformation/employmentInformation/employmentInformation",
				"style": {
					"navigationBarTitleText": "就业信息"
				}
			},
			{  // 通知公告
				"path": "notice/notice/noticeList",
				"style": {
					"navigationBarTitleText": "通知公告"
				}
			},
			{  // 详情
				"path": "notice/notice/noticeDetail",
				"style": {
					"navigationBarTitleText": "详情"
				}
			},
			{  // 缴费列表
				"path": "pay/pay/payList",
				"style": {
					"navigationBarTitleText": "我的缴费"
					// "navigationStyle":"custom"
				}
			}
			
		]
	}],
	// 预加载配置
	"preloadRule": {
		"page/smartrtStreet_page/equipmentInspection/equipmentInspection/equipmentRepairService": {// 分包中的主目录
			"network": "all",// 默认all，即适用所有网络
			"packages": ["page/smartrtStreet_page"]// 进入页面后预下载分包的 root 或 name。__APP__ 表示主包。
		},
		"page/smartrtStreet_page/login/accountNumber/accountNumber": {// 分包中的主目录
			"network": "all",// 默认all，即适用所有网络
			"packages": ["page/smartrtStreet_page"]// 进入packagePages/index/index后，加载整个项目
		},
		"page/smartrtStreet_page/publicFeedback/publicFeedback/publicFeedback": {// 分包中的主目录
			"network": "all",// 默认all，即适用所有网络
			"packages": ["page/smartrtStreet_page"]// 进入packagePages/index/index后，加载整个项目
		},
		"page/smartrtStreet_page/employmentInformation/employmentInformation/employmentInformation": {// 分包中的主目录
			"network": "all",// 默认all，即适用所有网络
			"packages": ["page/smartrtStreet_page"]// 进入packagePages/index/index后，加载整个项目
		},
		"page/smartrtStreet_page/notice/notice/noticeList": {// 公告列表
			"network": "all",// 默认all，即适用所有网络
			"packages": ["page/smartrtStreet_page"]// 进入packagePages/index/index后，加载整个项目
		},
		"page/smartrtStreet_page/notice/notice/noticeDetail": {// 公告详情
			"network": "all",// 默认all，即适用所有网络
			"packages": ["page/smartrtStreet_page"]// 进入packagePages/index/index后，加载整个项目
		},
		"page/smartrtStreet_page/pay/pay/payList": {// 缴费列表
			"network": "all",// 默认all，即适用所有网络
			"packages": ["page/smartrtStreet_page"]// 进入packagePages/index/index后，加载整个项目
		}
	},
	
	// 全局配置
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "智慧园区小程序",
		"navigationBarBackgroundColor": "#FFFFFF",
		"backgroundColor": "#F8F8F8",
		
		"usingComponents": {
		    "van-row": "/wxcomponents/vant/dist/row/index",
		    "van-col": "/wxcomponents/vant/dist/col/index",
		    "van-icon": "/wxcomponents/vant/dist/icon/index",
